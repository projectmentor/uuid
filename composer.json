{
    "name": "projectmentor/uuid",
    "description": "Static UUID utilities.",
    "type": "library",
    "authors": [
        {
            "name": "David Faith",
            "email": "david@projectmentor.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "6.5.x-dev",
        "orchestra/testbench": "3.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "projectmentor\\UUID\\": "src"
        }
    },
    "autoload-dev": {
